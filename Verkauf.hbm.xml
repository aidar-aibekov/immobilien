<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entities.Verkauf" table="verkauf" schema="immobilien">
        <id name="id_verkauf">
            <column name="id_verkauf" sql-type="int"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(255)" length="255" not-null="true"/>
        </property>
        <property name="datum">
            <column name="datum" sql-type="varchar(255)" length="255" not-null="true"/>
        </property>
        <property name="telefonnummer">
            <column name="telefonnummer" sql-type="varchar(255)" length="255" not-null="true"/>
        </property>
        <many-to-one name="haus" class="entities.Haus" fetch="select">
            <column name="haus_id" not-null="true" />
        </many-to-one>

    </class>
</hibernate-mapping>